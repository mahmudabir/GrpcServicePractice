<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <LangVersion>preview</LangVersion>
<!--        <Protobuf_AdditionalImportDirs>$(MSBuildThisFileDirectory)Protos</Protobuf_AdditionalImportDirs>-->
    </PropertyGroup>

    <ItemGroup>
        <Protobuf Include="Protos\greet.proto" GrpcServices="Server" AdditionalImportDirs="Protos" />
        <Protobuf Include="Protos\test.proto" GrpcServices="Server" AdditionalImportDirs="Protos" />
    </ItemGroup>


    <ItemGroup>
<!--        <PackageReference Include="Google.Api.CommonProtos" Version="2.17.0"/>-->
        <PackageReference Include="Grpc.AspNetCore" Version="2.64.0"/>
        <PackageReference Include="Microsoft.AspNetCore.Grpc.JsonTranscoding" Version="9.0.9"/>
        <PackageReference Include="Microsoft.AspNetCore.Grpc.Swagger" Version="0.9.9"/>
        <PackageReference Include="Riok.Mapperly" Version="4.2.1"/>
    </ItemGroup>

</Project>
